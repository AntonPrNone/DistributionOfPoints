<Window x:Class="DistributionOfPoints.StartWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DistributionOfPoints"
        mc:Ignorable="d"
        Title="StartWindow" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Window.Background>
        <RadialGradientBrush>
            <GradientStop Color="Black" Offset="1"/>
            <GradientStop Color="#FF131313" Offset="0"/>
        </RadialGradientBrush>
    </Window.Background>
    <Grid>
        <Button x:Name="DistributionOfPoints_Button" Content="DistributionOfPoints" HorizontalAlignment="Center" Margin="0,84,0,0" VerticalAlignment="Top" Height="30" Width="157" Click="DistributionOfPoints_Button_Click"/>

    </Grid>
</Window>
